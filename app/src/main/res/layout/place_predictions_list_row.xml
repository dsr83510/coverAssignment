<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/place_prediction_list_row"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="7dp"
    android:padding="5dp"
    android:orientation="vertical">

    <TextView
        android:id="@+id/place_prediction_list_row_addressLine1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        tools:text="13422 6TH Ave SW"
        android:textStyle="bold"></TextView>

    <TextView
        android:id="@+id/place_prediction_list_row_addressLine2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        tools:text="Burien, Washington USA">

    </TextView>

</LinearLayout>